<%= render "layouts/errors", :object => @property %>

<%= form_for @property do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div><br>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
